{
  "name": "physical-ai-curriculum-book",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "validate-links": "find docs -name '*.md' -exec grep -l '\\[.*\\]\\(.*\\)' {} \\; | xargs -I {} sh -c 'echo \"Validating links in {}\"; grep -oE \"\\[.*\\]\\(.*\\)\" {} | grep -oE \"\\(.*\\)\" | sed \"s/[()]//g\"'",
    "validate-build": "npm run build && echo 'Build validation passed!'"
  },
  "dependencies": {
    "@docusaurus/core": "3.1.0",j
    "@docusaurus/module-type-aliases": "3.1.0",
    "@docusaurus/preset-classic": "3.1.0",
    "@docusaurus/types": "3.1.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.1.0",
    "@docusaurus/preset-classic": "3.1.0",
    "@docusaurus/tsconfig": "3.1.0",
    "@docusaurus/types": "3.1.0",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
